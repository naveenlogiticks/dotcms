  display: none;
  font-size: 1.4em;
  text-align: center;
  margin-bottom: 0.1em;
  /* transparent black background, or fallback to all black (oldie) */
  /* background-color-with-alpha */
  background-color: #000000;
  background-color: rgba(0, 0, 0, 0.5);
}
